BA;AACA,SAAA,IAAA,GAAA,cAAA,CAAA,OAAA,CAAA,MAAA,IAAA,IAAA,CAAA,KAAA,CAAA,cAAA,CAAA,OAAA,CAAA,MAAA,CAAA,CAAA,GAAA,EAAA;AACA,SAAA,IAAA;AACA,GAnBA;AAoBA,EAAA,OAAA,EAAA;AACA,IAAA,IAAA,EAAA,IADA;AAEA,IAAA,WAFA,uBAEA,EAFA,EAEA;AACA,WAAA,OAAA,CAAA,OAAA,CAAA;AAAA,QAAA,IAAA,EAAA,cAAA;AAAA,QAAA,KAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAAA,OAAA;AACA,KAJA;AAKA,IAAA,kBALA,8BAKA,EALA,EAKA;AACA,WAAA,WAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,EAAA;AACA,KARA;AASA,IAAA,IATA,kBASA;AAAA;;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,aAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAFA;AAIA,MAAA,GAAA,CAAA,GAAA,CAAA,sBAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;;AAEA,QAAA,KAAA,CAAA,SAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA;AACA,cAAA,CAAA,IAAA,CAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,GAAA,CAAA,EAAA,CAAA;AACA,WAFA,MAEA;AACA,YAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AACA;AACA,SAPA;AAQA,OAXA;AAaA,MAAA,GAAA,CAAA,GAAA,CAAA,kBAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA;;AAEA,QAAA,KAAA,CAAA,KAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA;AACA,cAAA,CAAA,IAAA,CAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,GAAA,CAAA,EAAA,CAAA;AACA,WAFA,MAEA;AACA,YAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AACA;AACA,SAPA;AAQA,OAXA;AAaA,MAAA,GAAA,CAAA,GAAA,CAAA,eAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,EAAA;;AACA,QAAA,KAAA,CAAA,SAAA,CAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,EAAA;AACA,OAJA;AAKA,KA7CA;AA8CA,IAAA,SA9CA,qBA8CA,UA9CA,EA8CA;AAAA;;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,2BAAA,UAAA,EAAA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,OAAA,EAAA,KAAA,OADA;AAEA,UAAA,QAAA,EAAA,KAAA;AAFA;AADA,OAAA,EAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;;AAEA,QAAA,MAAA,CAAA,SAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA;AACA,cAAA,CAAA,IAAA,CAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,GAAA,CAAA,EAAA,CAAA;AACA,WAFA,MAEA;AACA,YAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AACA;AACA,SAPA;AAQA,OAjBA;AAkBA,KAjEA;AAkEA,IAAA,gBAlEA,4BAkEA,QAlEA,EAkEA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,SAAA,CAAA,KAAA,UAAA,CAAA,CAAA,EAAA,EAAA;AACA,KArEA;AAsEA,IAAA,mBAtEA,+BAsEA,OAtEA,EAsEA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,SAAA,CAAA,KAAA,UAAA,CAAA,CAAA,EAAA,EAAA;AACA;AAzEA;AApBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin-top: 10px\">\r\n      <el-carousel height=\"400px\">\r\n        <el-carousel-item v-for=\"item in imgList\" :key=\"item.id\">\r\n          <a target=\"_blank\" :href=\"item.url\"><img style=\"width: 100%\" :src='item.img' alt=\"\"></a>\r\n        </el-carousel-item>\r\n      </el-carousel>\r\n    </div>\r\n\r\n    <!--    其他-->\r\n    <div style=\"margin-top: 10px\">\r\n      <el-card>\r\n        <el-row :gutter=\"10\">\r\n          <!--          分类-->\r\n          <el-row :span=\"4\">\r\n            <div style=\"border: 1px dashed #ccc; text-align: center; border-bottom: none\" class=\"radio\">\r\n              <el-radio-group size=\"medium\"  class=\"category-item\" @click.native=\"getGoodsByCategory(item.id)\" v-for=\"item in categories\" :key=\"item.id\"\r\n                   :class=\"{ active: (activeIndex === item.id)}\"><el-radio-button  label=\"item.name\" class=\"radioitem\" >{{item.name }}</el-radio-button>\r\n              </el-radio-group>\r\n\r\n            </div>\r\n          </el-row>\r\n\r\n          <el-col :span=\"20\">\r\n            <!--          分类商品-->\r\n            <div style=\"min-height: 150px\" class=\"category\">\r\n              <el-row :gutter=\"10\">\r\n                <el-col :span=\"6\" v-for=\"item in tableData\" :key=\"item.id\">\r\n                  <div style=\"margin-bottom: 10px; margin-outside:10px;cursor: pointer\"  @click=\"goodsDetail(item.id)\">\r\n                    <div style=\"padding: 5px\"><el-image :src=\"item.imgs[0]\" style=\"width: 100%; height: 100px\" fit=\"contain\"></el-image></div>\r\n                    <div style=\"padding: 5px; height: 20px; text-align: center; font-size: 12px; overflow: hidden\">\r\n                      <el-tooltip :content=\"item.name\" placement=\"bottom\" effect=\"light\">\r\n                        <span class=\"item-title\">{{ item.name }}</span>\r\n                      </el-tooltip>\r\n                    </div>\r\n                    <div style=\"padding: 5px; text-align: center; color: red; font-size: 12px\">\r\n                      ￥ {{ item.price }}\r\n                    </div>\r\n                  </div>\r\n                </el-col>\r\n              </el-row>\r\n            </div>\r\n\r\n            <!--      分页-->\r\n            <div style=\"margin-top: 20px\">\r\n              <el-pagination\r\n                  small\r\n                  @size-change=\"handleSizeChange\"\r\n                  @current-change=\"handleCurrentChange\"\r\n                  :current-page=\"pageNum\"\r\n                  :page-size=\"pageSize\"\r\n                  :page-sizes=\"[4, 8, 12]\"\r\n                  layout=\" prev, pager, next\"\r\n                  :total=\"total\"\r\n              >\r\n              </el-pagination>\r\n            </div>\r\n\r\n          </el-col>\r\n        </el-row>\r\n      </el-card>\r\n\r\n    </div>\r\n\r\n\r\n    <div style=\"margin-top: 10px\">\r\n      <el-row :gutter=\"10\">\r\n        <!--    推荐商品-->\r\n        <el-col :span=\"18\">\r\n          <el-card>\r\n            <div style=\"padding: 10px 0; border-bottom: 2px solid orangered; font-size: 20px; color: orangered\">推荐商品</div>\r\n            <div style=\"padding: 10px 0\">\r\n              <el-row :gutter=\"10\">\r\n                <el-col :span=\"6\" v-for=\"item in recommend\" :key=\"item.id\" style=\"margin-bottom: 10px\">\r\n                  <div style=\"border: 1px solid #ccc; cursor: pointer; padding: 10px\"  @click=\"goodsDetail(item.id)\">\r\n                    <el-image :src=\"item.imgs[0]\" style=\"width: 100%; height: 100px\" fit=\"contain\"></el-image>\r\n                    <div style=\"padding: 5px; height: 20px; text-align: center; font-size: 12px; overflow: hidden\">\r\n                      <el-tooltip :content=\"item.name\" placement=\"bottom\" effect=\"light\">\r\n                        <span class=\"item-title\">{{ item.name }}</span>\r\n                      </el-tooltip>\r\n                    </div>\r\n                    <div style=\"padding: 5px; text-align: center; color: red; font-size: 12px\">\r\n                      ￥ {{ item.price }}\r\n                    </div>\r\n                  </div>\r\n\r\n                </el-col>\r\n              </el-row>\r\n            </div>\r\n          </el-card>\r\n        </el-col>\r\n\r\n<!--        销售排行-->\r\n        <el-col :span=\"6\">\r\n          <el-card>\r\n            <div style=\"padding: 10px 0; border-bottom: 2px solid dodgerblue; font-size: 20px; color: dodgerblue\">销量排行榜</div>\r\n\r\n            <div style=\"padding: 10px 0\">\r\n              <el-row v-for=\"(item, index) in sales\" :key=\"item.id\">\r\n                <div style=\"padding: 10px 0; cursor: pointer;\" @click=\"goodsDetail(item.id)\">\r\n                  <el-col :span=\"2\" style=\"padding-top: 5px\">\r\n                    <span v-if=\"index === 0\" style=\"color: gold\">{{ index + 1 }}</span>\r\n                    <span v-if=\"index === 1\" style=\"color: silver\">{{ index + 1 }}</span>\r\n                    <span v-if=\"index === 2\" style=\"color: coral\">{{ index + 1 }}</span>\r\n                    <span v-if=\"index > 2\">{{ index + 1 }}</span>\r\n                  </el-col>\r\n                  <el-col :span=\"10\"><el-image :src=\"item.imgs[0]\" style=\"width: 100%; height: 80px\" fit=\"contain\"></el-image></el-col>\r\n                  <el-col :span=\"12\">\r\n                    <div style=\"overflow: hidden; height: 80px; font-size: 12px\">\r\n                      {{ item.description }}\r\n                    </div>\r\n                  </el-col>\r\n                </div>\r\n              </el-row>\r\n            </div>\r\n          </el-card>\r\n        </el-col>\r\n      </el-row>\r\n\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport API from \"@/utils/request\";\r\nimport {left} from \"core-js/internals/array-reduce\";\r\n\r\nconst url = \"/api/video/\"\r\n\r\nexport default {\r\n  name: \"Home\",\r\n  data() {\r\n    return {\r\n      sales: [],\r\n      recommend: [],\r\n      tableData: [],\r\n      activeIndex: 0,\r\n      imgList: [],\r\n      user: {},\r\n      categories: [],\r\n      pageNum: 1,\r\n      pageSize: 8,\r\n      total: 0\r\n    };\r\n  },\r\n  created() {\r\n    this.user = sessionStorage.getItem(\"user\") ? JSON.parse(sessionStorage.getItem(\"user\")) : {}\r\n    this.load()\r\n  },\r\n  methods: {\r\n    left,\r\n    goodsDetail(id) {\r\n      this.$router.replace({path: '/front/goods', query: {id: id}})\r\n    },\r\n    getGoodsByCategory(id) {\r\n      this.activeIndex = id\r\n      this.loadTable(id)\r\n    },\r\n    load() {\r\n      API.get(\"/api/banner\").then(res => {\r\n        this.imgList = res.data\r\n      })\r\n\r\n      API.get(\"/api/goods/recommend\").then(res => {\r\n        this.recommend = res.data\r\n\r\n        this.recommend.forEach(item => {\r\n          // 处理下表格的图片显示\r\n          if (!item.imgs) {\r\n            item.imgs = ['']\r\n          } else {\r\n            item.imgs = JSON.parse(item.imgs)\r\n          }\r\n        })\r\n      })\r\n\r\n      API.get(\"/api/goods/sales\").then(res => {\r\n        this.sales = res.data\r\n\r\n        this.sales.forEach(item => {\r\n          // 处理下表格的图片显示\r\n          if (!item.imgs) {\r\n            item.imgs = ['']\r\n          } else {\r\n            item.imgs = JSON.parse(item.imgs)\r\n          }\r\n        })\r\n      })\r\n\r\n      API.get(\"/api/category\").then(res => {\r\n        this.categories = res.data\r\n        this.activeIndex = res.data[0].id\r\n        this.loadTable(res.data[0].id)\r\n      })\r\n    },\r\n    loadTable(categoryId) {\r\n      API.get(\"/api/goods/byCategory/\" + categoryId, {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n\r\n        this.tableData.forEach(item => {\r\n          // 处理下表格的图片显示\r\n          if (!item.imgs) {\r\n            item.imgs = ['']\r\n          } else {\r\n            item.imgs = JSON.parse(item.imgs)\r\n          }\r\n        })\r\n      })\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      this.pageSize = pageSize;\r\n      this.loadTable(this.categories[0].id)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.pageNum = pageNum;\r\n      this.loadTable(this.categories[0].id)\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.active {\r\n  color: red !important;\r\n}\r\n\r\n.category-item {\r\n  padding: 20px 0;\r\n  border-bottom: 1px dashed #ccc;\r\n  color: #666;\r\n  cursor: pointer;\r\n  text-align: center;\r\n}\r\n\r\n.line1 {\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n}\r\n.radio ::v-deep .el-radio-button__inner{\r\n  background-color: #ff4500;\r\n  color:white;\r\n  block-width: 250px;\r\n  border-color: gray;\r\n  border-radius: 500px;\r\n}\r\n\r\n.radioitem ::v-deep .el-radio-button__inner{\r\n  width: 210px;\r\n  text-align: center;\r\n}\r\n.category{\r\n  text-align: center;\r\n  margin-left: 100px;\r\n}\r\n\r\n\r\n.item-title {\r\n  cursor: pointer;\r\n}\r\n.item-title:hover {\r\n  color: orangered;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/front"}]}